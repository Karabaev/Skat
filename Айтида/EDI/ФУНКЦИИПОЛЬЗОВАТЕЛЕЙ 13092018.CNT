#command INSERT INTO qreport_functions (CODE,NAME,DESCRIPTION_,TEXT_,NOTE,F_SYSTEM,F_LOCAL,PARENT,FOLDER,POSORDER,CACCESS,TACCESS,NOTAVAIL,MAINDB,MAINIC,FARC,UPDATED,EDIT_DAT) VALUES ('','ВырезатьСимволыИзСтроки','ВЫРЕЗАТЬСИМВОЛЫИЗСТРОКИ (ИСХОДНАЯСТРОКА, СИМВОЛЫДЛЯУДАЛЕНИЯ)','НомерПервогоСимволаДляУдаления = ПОЗИЦИЯР(ИСХОДНАЯСТРОКА, СИМВОЛЫДЛЯУДАЛЕНИЯ);/##CHR(13)##//##CHR(10)##/ВЫХОДНАЯСТРОКА = "";/##CHR(13)##//##CHR(10)##/if(НомерПервогоСимволаДляУдаления == 0) /##CHR(13)##//##CHR(10)##/	return ИСХОДНАЯСТРОКА;/##CHR(13)##//##CHR(10)##/ВЫХОДНАЯСТРОКА = ЗАМЕНИТЬ(ИСХОДНАЯСТРОКА, СИМВОЛЫДЛЯУДАЛЕНИЯ, "", 0, 1);/##CHR(13)##//##CHR(10)##/return ВЫХОДНАЯСТРОКА;/##CHR(13)##//##CHR(10)##/','',0,0,0.00000000,0,530.00000000,'','',0,'',0.00000000,0,1,'20.02.2018 14:08:51')
